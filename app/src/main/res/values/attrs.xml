<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="JoyStickView">
        <attr name="JoyStickStyle">
            <enum name="JoyStickStylePower" value="0" />
            <enum name="JoyStickStyleRanger" value="1" />
        </attr>
    </declare-styleable>



    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color"/>
        <attr name="innerColor" format="color"/>
        <attr name="boderWidth" format="dimension"/>
        <attr name="stepTextColor" format="color"/>
        <attr name="setpTextSize" format="dimension"/>
    </declare-styleable>


    <!--仿QQ运动步数进度效果-->
    <declare-styleable name="QQStyepView">
        <attr name="cmOuterColor" format="color"></attr>
        <attr name="cmInnerColor" format="color"></attr>
        <attr name="cmBorderWidth" format="dimension"></attr>
        <attr name="cmStepTextSize" format="dimension"></attr>
        <attr name="cmStepTextColor" format="color"></attr>
    </declare-styleable>

    <!--自定义TextView-->
    <declare-styleable name="CMTextView">
        <attr name="cmTextColor" format="color"></attr>
        <attr name="cmText" format="string"></attr>
        <attr name="cmTextSize" format="dimension"></attr>
        <attr name="cmTextMaxLength" format="integer"></attr>
    </declare-styleable>



    <declare-styleable name="ColorTrackTextView">
        <attr name="cmChangeTextColor" format="color"></attr>
        <attr name="cmOriginTextColor" format="color"></attr>
    </declare-styleable>



    <declare-styleable name="CorloTrackView">
        <attr name="changeTextColor" format="color"></attr>
        <attr name="originTextColor" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="CMProgressView">
        <attr name="cmProgressOuterColor" format="color"></attr>
        <attr name="cmProgressInnerColor" format="color"></attr>
        <attr name="cmProgressBorderWidth" format="dimension"></attr>
        <attr name="cmProgressTextSize" format="dimension"></attr>
        <attr name="cmProgressTextColor" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="CMRatingBarView">
        <attr name="cmRatingBarStartNormal" format="reference"></attr>
        <attr name="cmRatingBarStartFocus" format="reference"></attr>
        <attr name="cmRatingBarGradeNumber" format="integer"></attr>
    </declare-styleable>

    <!--字母索引列表-->
    <declare-styleable name="CMLetterSiderView">
        <attr name="cmLetterSiderChooseTextColor" format="color"></attr>
        <attr name="cmLetterSiderChooseTextSize" format="dimension"></attr>
        <attr name="cmLetterSiderNormalTextColor" format="color"></attr>
        <attr name="cmLetterSiderNormalTextSize" format="dimension"></attr>
    </declare-styleable>

    <!--仿酷狗侧拉-->
    <declare-styleable name="CMSlidingMenuView">
        <attr name="cmSlidingMenuRightMargin" format="dimension"></attr>
    </declare-styleable>

    <!--仿qq侧拉-->
    <declare-styleable name="CMQQSlidingMenuView">
        <attr name="cmQQSlidingMenuRightMargin" format="dimension"></attr>
    </declare-styleable>




    <declare-styleable name="RadarWaveView">

        <attr name="waveColor" format="color" />
        <attr name="waveAmplitude" format="dimension" />
        <attr name="waveSpeed" format="float" />
        <attr name="waveStartPeriod" format="float" />
        <attr name="waveStart" format="boolean" />
        <attr name="waveFillTop" format="boolean" />
        <attr name="waveFillBottom" format="boolean" />

        <attr name="waveFillType" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
        </attr>
        <attr name="waveType" format="enum">
            <enum name="sin" value="0" />
            <enum name="cos" value="1" />
        </attr>

    </declare-styleable>



    <declare-styleable name="WaveLoadingView">
        <attr name="wlv_borderWidth" format="dimension"/>
        <attr name="wlv_borderColor" format="color"/>
        <attr name="wlv_progressValue" format="integer"/>
        <attr name="wlv_shapeType" format="enum">
            <enum name="triangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="square" value="2" />
            <enum name="rectangle" value="3" />
        </attr>

        <attr name="wlv_triangle_direction" format="enum">
            <enum name="north" value="0" />
            <enum name="south" value="1" />
            <enum name="east" value="2" />
            <enum name="west" value="3" />
        </attr>

        <attr name="wlv_round_rectangle" format="boolean"/>
        <attr name="wlv_round_rectangle_x_and_y" format="integer"/>

        <attr name="wlv_waveColor" format="color"/>
        <attr name="wlv_wave_background_Color" format="color"/>
        <attr name="wlv_waveAmplitude" format="float"/>

        <attr name="wlv_titleTop" format="string"/>
        <attr name="wlv_titleCenter" format="string"/>
        <attr name="wlv_titleBottom" format="string"/>

        <attr name="wlv_titleTopSize" format="dimension"/>
        <attr name="wlv_titleCenterSize" format="dimension"/>
        <attr name="wlv_titleBottomSize" format="dimension"/>

        <attr name="wlv_titleTopColor" format="color"/>
        <attr name="wlv_titleCenterColor" format="color"/>
        <attr name="wlv_titleBottomColor" format="color"/>

        <attr name="wlv_titleTopStrokeColor" format="color"/>
        <attr name="wlv_titleTopStrokeWidth" format="dimension"/>

        <attr name="wlv_titleBottomStrokeColor" format="color"/>
        <attr name="wlv_titleBottomStrokeWidth" format="dimension"/>

        <attr name="wlv_titleCenterStrokeColor" format="color"/>
        <attr name="wlv_titleCenterStrokeWidth" format="dimension"/>
    </declare-styleable>



    <declare-styleable name="LikeView">
        <attr name="mmove_distance" format="integer"/>
        <attr name="mstart_move_position" format="integer"/>

        <attr name="mtext" format="string"/>
        <attr name="mtext_color" format="color"/>
        <attr name="mtext_size" format="integer"/>

        <attr name="mfrom_alpha" format="float"/>
        <attr name="mto_alpha" format="float"/>
        <attr name="mduration" format="integer"/>

        <attr name="mlike_img" format="reference"/>
        <attr name="mainm_img" format="reference"/>

        <attr name="manim_type" format="enum">
            <enum name="mimg" value="0"/>
            <enum name="mtxt" value="1"/>
        </attr>
    </declare-styleable>



    <declare-styleable name="CustomVolumControlBar">
        <attr name="progressColor" />
        <attr name="backgroundColor" />
        <attr name="circleWidth" />
        <attr name="dotCount" />
        <attr name="splitSize" />
        <attr name="centerPic" />
        <attr name="centralAngle" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="textVisible" />
    </declare-styleable>

    <declare-styleable name="VolumeControlView">
        <attr name="bgArcColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="split" format="integer"/>
        <attr name="count" format="integer"/>
        <attr name="arcWidth" format="integer"/>
        <attr name="centerImage" format="reference"/>
    </declare-styleable>
</resources>